        "@ionic-native/firebase": "^4.17.0",
        "@ionic-native/geolocation": "^4.17.0",
        "@ionic-native/in-app-browser": "^4.17.0",
        "@ionic-native/keyboard": "4.15.0",
        "@ionic-native/native-geocoder": "^4.17.0",
        "@ionic-native/network": "^4.17.0",
        "@ionic-native/pro": "4.15.0",
        "@ionic-native/splash-screen": "4.15.0",
        "@ionic-native/status-bar": "4.15.0",
        "@ionic/pro": "1.0.20",
        "@ionic/storage": "2.2.0",
        "@types/googlemaps": "^3.30.16",
        "angularfire2": "^5.1.0",
        "build": "0.1.4",
        "cordova-android": "7.1.4",
        "cordova-plugin-console": "^1.1.0",
        "cordova-plugin-device": "^1.1.7",
        "cordova-plugin-firebase": "2.0.5",
        "cordova-plugin-geolocation": "^4.0.1",
        "cordova-plugin-inappbrowser": "3.0.0",
        "cordova-plugin-nativegeocoder": "^3.1.3",
        "cordova-plugin-network-information": "2.0.1",
        "cordova-plugin-splashscreen": "^4.1.0",
        "cordova-plugin-statusbar": "^2.4.2",
        "cordova-plugin-whitelist": "^1.3.3",
        "cordova-sqlite-storage": "^2.5.2",
        "firebase": "^5.5.9",
        "ionic-angular": "3.9.2",
        "ionic-plugin-keyboard": "^2.2.1",
        "ionicons": "^4.4.8",
        "moment": "^2.22.2",
        "promise-polyfill": "^8.1.0",
        "rxjs": "^6.3.3",
        "rxjs-compat": "^6.3.3",
        "sw-toolbox": "^3.6.0",
        "zone.js": "0.8.18"
    },
    "devDependencies": {
        "@ionic/app-scripts": "3.2.0",
        "@ionic/lab": "1.0.9",
        "html-minifier": "^3.5.21",
        "purify-css": "^1.2.5",
        "typescript": "2.4.2"
    },
    "description": "Air Conditioning Service Application",
